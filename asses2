#include<stdio.h>
void main()
{
	int i,j;
	for(i=1;i<=5;i++)
	{
		printf("\n");
		for(j=1;j<=5;j++)
		{
			if(j<=i)
			printf("%d",j);
			else
			printf(" ");
		}
	}
	printf("\n");
	for(i=4;i>=1;i--)
	{
		for(j=1;j<5;j++)
		{
			if(i>=j)
			printf("%d",j);
			else
			printf(" ");
		}
		printf("\n");
	}
}
